Algoritmo "consumo"

Var

   distancia, consumo, combustivel: real

Inicio

      escreva("Distancia percorrida: ")
      leia(distancia)
      escreva("Combustível gasto: ")
      leia(combustivel)
      
      consumo <- distancia / combustivel
      
      escreva("Consumo medio: ", consumo:1:3)

Fimalgoritmo